
public class ReverseString{
	String givenString;
	String newString;
	
public ReverseString(String givenString){
	
	this.givenString =givenString;
	this.newString="";
	
	for(int i=givenString.length();i>0;i=i-1){
		this.newString=newString+(givenString.charAt(i-1));
	}
	
	System.out.println(newString);
}

public static String getReversedstring(){
	return this.newString;
}

public static void main(String[] args){
	new ReverseString("dmLKv");
	System.out.println(getReversedstring());
}
}
