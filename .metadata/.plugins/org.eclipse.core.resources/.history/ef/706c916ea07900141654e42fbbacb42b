
import java.util.*;

public class NeedleInHay {

	int indexOfstring;
public static void main(String args[]){
	String needle="or";
	String[] haystack1={"and","or","else","if"};
	
	
	for(int i=0;i<haystack1.length;i++){
		if(haystack1[i].compareTo(needle)<0){
			indexOfstring=i;
		}
	}
	System.out.println(indexOfstring);
}
}
